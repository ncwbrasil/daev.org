    }
}

#destaques .destaque2 h1 {
    font-size: 40px;
    font-weight: 900;
    line-height: 50px;
    text-transform: uppercase;
}

.bnn2 {
    width: 99%;
    display: table;
    height: 280px;
    text-align: center;
    border-radius: 15px;
    background: #2f91ce;
}

.bnn2 a {
    padding: 100px 0;
    color: #fff;
    font-size: 30px;
    font-weight: 700;
}


