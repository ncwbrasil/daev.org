}

@media screen and (max-width: 1200px) {
    #destaques {
        margin: 0 auto;
    }
}

#destaques .home_servicos {
    width: 100%;
    display: table;
}

#destaques .destaque1 .list {
    width: 96%;
    text-align: center;
    padding: 2%;
    border-radius: 15px;
    margin-bottom: 10px;
    font-size: 20px;
}

#destaques .destaque1 #verde {
    background: #bbd151;
}

#destaques .destaque1 #branco {
    background: #fff;
}

#destaques .destaque2 {
    padding: 2%;
    background: #2f91ce;
    border-radius: 15px;
    margin: 0 auto;
    width: 44%;
